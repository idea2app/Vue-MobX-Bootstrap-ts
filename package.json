{
  "name": "vue-prime-ts",
  "version": "2.2.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "echarts-jsx": "^1.1.0",
    "idb-keyval": "^6.2.1",
    "koajax": "^1.0.0",
    "native-file-system-adapter": "^3.0.1",
    "primevue": "^3.51.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "web-utility": "^4.3.0"
  },
  "devDependencies": {
    "@octokit/openapi-types": "^22.0.1",
    "@parcel/transformer-vue": "~2.12.0",
    "@vue/tsconfig": "^0.5.1",
    "buffer": "^6.0.3",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "parcel": "~2.12.0",
    "prettier": "^3.2.5",
    "typescript": "~5.4.5",
    "vue-tsc": "^2.0.12"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.{html,md,css,less,json,yml,js,ts,mts,vue}": "prettier --write"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead, IE 11",
  "scripts": {
    "prepare": "husky",
    "clean": "rm -rf .parcel-cache/ dist/",
    "start": "npm run clean  &&  parcel src/index.html",
    "build": "npm run clean  &&  vue-tsc --noEmit  &&  parcel build src/index.html --public-url .",
    "test": "lint-staged"
  }
}
